{
  "prettier.eslintIntegration": false,
  "colors.filetypes": ["*"],
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "yaml",
    "yml",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "css",
    "html"
  ],
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "flow.stopFlowOnExit": false,
  "javascript.validate.enable": true,
  "javascript.suggest.completeFunctionCalls": false,
  "typescript.suggest.completeFunctionCalls": false,
  "javascript.suggest.names": false,
  "typescript.suggestionActions.enabled": true,
  "tsserver.enable": true,
  "languageserver": {
    "kotlin": {
      "command": "~/lsp/kotlin/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "cSpell.showCommandsInEditorContextMenu": false,
  "cSpell.diagnosticLevel": "Hint",
  "diagnostic.locationlistLevel": "information",
  "diagnostic.messageLevel": "information",
  "diagnostic.hintSign": "",
  "cSpellExt.enableDictionaries": ["german"],
  "coc.preferences.jumpCommand": "tabe",
  "cSpell.enabledLanguageIds": [
    "vim",
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
