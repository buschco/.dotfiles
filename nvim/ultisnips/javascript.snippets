# A valid snippet should starts with:
#
#		snippet trigger_word [ "description" [ options ] ]
#
# and end with:
#
#		endsnippet
#
# Snippet options:
#
#		b - Beginning of line.
#		i - In-word expansion.
#		w - Word boundary.
#		r - Regular expression
#		e - Custom context snippet
#		A - Snippet will be triggered automatically, when condition matches.
#
# Basic example:
#
#		snippet emitter "emitter properties" b
#		private readonly ${1} = new Emitter<$2>()
#		public readonly ${1/^_(.*)/$1/}: Event<$2> = this.$1.event
#		endsnippet
#
# Online reference: https://github.com/SirVer/ultisnips/blob/master/doc/UltiSnips.txt
snippet conso  "log your stuff" b
console.log('cbu', ${1})
endsnippet

snippet action "simple action thunk" b
function ${1} () {
	return (dispatch: mixed => any, getState: AppStateFunc) => {
		
	}
}
endsnippet

snippet selector "simple selector" b
type ${1} = {| 

|}

const get${1}: (state: AppState) => ${1} =
  createSelector(
    'get${1}',
    [],
    (): ${1} => {
      return { };
    }
  );
endsnippet

snippet okeys "convert object to array" b
Object.keys(${1}).
endsnippet
